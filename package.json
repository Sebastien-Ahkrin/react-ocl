{
  "name": "react-ocl",
  "version": "0.1.1",
  "description": "React components integrating OpenChemLib",
  "keywords": [
    "react",
    "react-component",
    "ocl",
    "openchemlib"
  ],
  "files": [
    "lib",
    "lib-es6"
  ],
  "main": "lib/index.js",
  "module": "lib-es6/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/neptunejs/react-ocl.git"
  },
  "homepage": "https://github.com/neptunejs/react-ocl#readme",
  "bugs": "https://github.com/neptunejs/react-ocl/issues",
  "author": "MichaÃ«l Zasso <targos@protonmail.com>",
  "scripts": {
    "compile": "babel src --out-dir lib --ignore __tests__ --quiet",
    "compile-es6": "cross-env BABEL_ENV=es6 babel src --out-dir lib-es6 --ignore __tests__ --quiet",
    "start": "cross-env BABEL_ENV=es6 webpack-dev-server --inline",
    "test": "run-s testonly eslint",
    "eslint": "eslint src examples",
    "eslint-fix": "eslint --fix src",
    "prepare": "run-p compile compile-es6",
    "testonly": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "openchemlib": ">=5.0.0",
    "react": "^15.4.2||^16.0.0",
    "react-dom": "^15.4.2||^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-config-cheminfo": "^1.10.0",
    "eslint-config-neptune-react": "^2.2.0",
    "eslint-plugin-jest": "^21.6.0",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.4",
    "npm-run-all": "^4.1.2",
    "openchemlib": "^5.5.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  }
}
