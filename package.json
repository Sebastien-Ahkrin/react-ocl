{
  "name": "react-ocl",
  "version": "1.0.1",
  "description": "React components integrating OpenChemLib",
  "keywords": [
    "react",
    "react-component",
    "ocl",
    "openchemlib"
  ],
  "files": [
    "lib",
    "lib-es6"
  ],
  "main": "lib/index.js",
  "module": "lib-es6/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/neptunejs/react-ocl.git"
  },
  "homepage": "https://github.com/neptunejs/react-ocl#readme",
  "bugs": "https://github.com/neptunejs/react-ocl/issues",
  "author": "MichaÃ«l Zasso <targos@protonmail.com>",
  "scripts": {
    "build-docs": "build-storybook -o docs",
    "compile": "babel src --out-dir lib --ignore src/**/__tests__ --quiet",
    "compile-es6": "cross-env BABEL_ENV=es6 babel src --out-dir lib-es6 --ignore src/**/__tests__ --quiet",
    "eslint": "eslint src stories",
    "eslint-fix": "eslint --fix src",
    "prepare": "npm run compile && npm run compile-es6",
    "storybook": "start-storybook -p 6006",
    "test": "npm run test-only && npm run eslint",
    "test-only": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "openchemlib": ">=5.0.0",
    "react": "^15.4.2||^16.0.0",
    "react-dom": "^15.4.2||^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.4",
    "@storybook/addon-info": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/addon-links": "^4.0.4",
    "@storybook/addons": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-cheminfo": "^1.18.0",
    "eslint-config-neptune-react": "^2.5.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "openchemlib": "^5.6.1",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-test-renderer": "^16.6.1"
  }
}
