    "react-dom": "^15.4.2||^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.5.0",
    "eslint-config-cheminfo": "^1.18.0",
    "eslint-config-neptune-react": "^2.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0",
    "npm-run-all": "^4.1.3",
    "openchemlib": "^5.6.0",
    "react": "^16.4.2",
